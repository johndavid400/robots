<title><%= title %></title>
<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
<%== meta_data_tags %>
<%== stylesheet_link_tag 'screen.css', 'jd.css', 'menu.css', 'anythingslider.css', 'anythingslider-ie.css', 'theme-minimalist-round.css' %>
<%= csrf_meta_tag %>
<%= javascript_include_tag 'jquery-1.6.4.min', 'jquery.validate.min.js', 'rails', 'application', 'jquery.anythingslider.min', 'jquery.anythingslider.fx.min', 'jquery.anythingslider.video.min', 'jquery.easing.1.2', :cache => 'jquery-core' %>
<% unless I18n.locale.to_s[0..1] == 'en' %>
  <%= javascript_include_tag ['localization/messages_', I18n.locale.to_s[0..1], '.js'].join %>
<% end %>
<%= yield :head %>
